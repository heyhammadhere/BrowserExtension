{
  "manifest_version": 3,
  "name": "Extension",
  "description": "Extension",
  "version": "1.0.0",
  "permissions": ["storage"],
  "action": {
    "default_popup": "./popup/index.html"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["./scripts/youtube.js"]
    }
  ]
}
